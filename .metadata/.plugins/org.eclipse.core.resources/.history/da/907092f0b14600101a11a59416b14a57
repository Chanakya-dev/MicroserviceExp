server:
  port: 8080

spring:
  application:
    name: ApiGateway

  cloud:
    gateway:
      routes:
        - id: demo1
          uri: http://localhost:8081
          predicates:
            - Path=/api/cvc/**

        - id: demo2
          uri: http://localhost:8082
          predicates:
            - Path=/api/svc/**
